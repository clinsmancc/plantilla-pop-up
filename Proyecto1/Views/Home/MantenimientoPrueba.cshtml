
@{
    ViewBag.Title = "MantenimientoPrueba";
}

<h2>Plantilla de pop up</h2>
<div>
    <div class="container">
        <div class="row">
            <h4 class="modal-title">
                <!--Aqui se coloca el contenido de entrada, el data-toggle, data-target hacen que el pop up sea posible-->
                <button type="button" data-toggle="modal" data-target="#myModal">Consultar</button>
            </h4>
        </div>
    </div>

    <div class="container">
        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">
                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                        </h4>
                    </div>
                    <div class="modal-body">
                        <asp:UpdatePanel ID="UpdatePanel1" runat="server">
                            <ContentTemplate>
                                <!--Aqui se coloca el contenido de salida-->
                            </ContentTemplate>
                        </asp:UpdatePanel>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
